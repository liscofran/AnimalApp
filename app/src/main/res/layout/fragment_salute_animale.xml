<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.Animali.Fragment_Salute_Animale">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/second_relative_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingClass">

            <EditText
                android:id="@+id/patologie"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/custom_button_v5"
                android:drawablePadding="10dp"
                android:elevation="10dp"
                android:hint="@string/nessuna"
                android:padding="20dp"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/orizzontale50"
                app:layout_constraintEnd_toStartOf="@+id/guideline14"
                app:layout_constraintLeft_toLeftOf="@id/guideline5"
                app:layout_constraintRight_toRightOf="@id/guideline14"
                app:layout_constraintStart_toStartOf="@+id/guideline15"
                app:layout_constraintTop_toTopOf="@+id/top17" />

            <TextView
                android:id="@+id/view_patologie"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/custom_button_v8"
                android:elevation="9dp"
                android:hint="@string/patologie"
                android:padding="10dp"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/orizzontale50"
                app:layout_constraintEnd_toStartOf="@+id/guideline14"
                app:layout_constraintLeft_toLeftOf="@id/guideline5"
                app:layout_constraintRight_toRightOf="@id/guideline14"
                app:layout_constraintStart_toStartOf="@+id/guideline15"
                app:layout_constraintTop_toTopOf="@+id/top15" />

            <EditText
                android:id="@+id/preferenze_cibo"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/custom_button_v5"
                android:elevation="10dp"
                android:hint="@string/nessuna"
                android:drawablePadding="10dp"
                android:padding="20dp"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/top90"
                app:layout_constraintEnd_toEndOf="@+id/patologie"
                app:layout_constraintStart_toStartOf="@+id/patologie"
                app:layout_constraintTop_toTopOf="@+id/orizzontale60" />

            <TextView
                android:id="@+id/view_preferenze_cibo"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/custom_button_v8"
                android:elevation="9dp"
                android:hint="@string/Preferenze_cibo"
                android:textStyle="bold"
                android:textAlignment="center"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/top90"
                app:layout_constraintEnd_toEndOf="@+id/patologie"
                app:layout_constraintStart_toStartOf="@+id/patologie"
                app:layout_constraintTop_toTopOf="@+id/orizzontale55" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/orizzontale55"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.55" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/orizzontale60"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.62" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/orizzontale50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.50" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.10" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.05" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/top17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.22" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/top15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.15" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/top10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.05" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.95" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/top90"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.90" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline669"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.96" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/top55"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.55" />

            <ImageButton
                android:id="@+id/back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:padding="5dp"
                android:src="@drawable/baseline_arrow_back_24"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/guideline5"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/salva"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/custom_button_v4"
                android:padding="5dp"
                android:src="@drawable/ic_baseline_save_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/guideline669" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline51"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.90" />

            <TextView
                android:id="@+id/Salute"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Salute"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/view_patologie"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/top10" />

        </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>